server {
 	listen 80 default_server;
         root /var/www/html;
 	index index.html index.htm;
 	server_name _;
 	location / {
 		try_files $uri $uri/ = 404;
 	}
 	location /prometheus {
 		proxy_pass http://localhost:9090;
 	}
}

server {
	listen 8080 default_server;

	location = /stub_status {
			stub_status;
	}
}
