server {
	listen 80 default_server;
	server_name _;

	location / {
		uwsgi_pass localhost:5000;
		include uwsgi_params;
	}

	location /grafana {
		proxy_pass http://michalkaiser-1:3000;
		proxy_set_header Host $http_host;
	}
}


server {
    listen 8080 default_server;

    location = /stub_status {
            stub_status;
    }
}
