apiVersion: 1

datasources:
  - name: Prometheus
    type: prometheus
    access: proxy
    # Access mode - proxy (server in the UI) or direct (browser in the UI).
    url: http://michalkaiser-2:9090/prometheus
    orgId: 1
    isDefault: true
    version: 1
    editable: false

  - name: influx-latency
    type: influxdb
    url: http://michalkaiser-2:8086
    access: proxy
    basicAuth: false
    database: latency
